<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.1.xsd"
       default-autowire="byName">

    <context:annotation-config />

    <bean name="MenuBarSeparatorAbout"
          class="org.geosdi.geoplatform.gui.configuration.menubar.MenubarSeparatorTool">
        <property name="order" value="90" />
    </bean>
                
    <bean name="aboutGeoPlatform"
          class="org.geosdi.geoplatform.gui.configuration.menubar.MenuBarClientTool">
        <property name="id" value="aboutGeoPlatform" />
        <property name="text" value="About" />
        <property name="enabled" value="true" />
        <property name="order" value="100" />
    </bean>

</beans>