<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.1.xsd"
       default-autowire="byName">

    <bean name="layerMenu"
          class="org.geosdi.geoplatform.gui.configuration.menubar.CheckMenuClientTool">
        <property name="id" value="layerMenu" />
        <property name="text" value="Layers" />
        <property name="checked" value="true" />
        <property name="enabled" value="true" />
        <property name="order" value="30" />
    </bean>
    
    <bean id="manageProjects"
          class="org.geosdi.geoplatform.gui.configuration.menubar.MenuBarClientTool">
        <property name="id" value="manageProjects" />
        <property name="text" value="Manage Projects" />
        <property name="enabled" value="true" />
        <property name="order" value="5" />
    </bean>
    
    <bean name="LayerProperties"
          class="org.geosdi.geoplatform.gui.configuration.menubar.MenuBarClientTool">
        <property name="id" value="LayerProperties" />
        <property name="text" value="Layer Properties" />
        <property name="enabled" value="true" />
        <property name="order" value="90" />
    </bean>
    

</beans>