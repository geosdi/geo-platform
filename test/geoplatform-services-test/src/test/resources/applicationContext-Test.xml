<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.2.xsd
            http://www.springframework.org/schema/util
            http://www.springframework.org/schema/util/spring-util-3.2.xsd"
       default-autowire="byName">

    <context:component-scan base-package="org.geosdi.geoplatform.configurator.cxf,
                            org.geosdi.geoplatform.cxf.bus,
                            org.geosdi.geoplatform.catalog.csw.beans,
                            org.geosdi.geoplatform.catalogws.beans,
                            org.geosdi.geoplatform.modelws.beans,
                            org.geosdi.geoplatform.ws.wfs.bean"/>

</beans>
